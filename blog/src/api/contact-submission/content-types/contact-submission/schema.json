{
  "kind": "collectionType",
  "collectionName": "contact_submissions",
  "info": {
    "singularName": "contact-submission",
    "pluralName": "contact-submissions",
    "displayName": "Contact Submission",
    "description": "Contact form submissions"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "subject": {
      "type": "string"
    },
    "message": {
      "type": "text",
      "required": true
    },
    "blog_post": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::blog-post.blog-post"
    },
    "read": {
      "type": "boolean",
      "default": false
    },
    "replied": {
      "type": "boolean",
      "default": false
    },
    "ipAddress": {
      "type": "string"
    }
  }
}

